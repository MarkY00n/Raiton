        </activity>
        <!-- Marty activity-->
        <activity android:name=".marty.MartyMainActivity" />
        <activity android:name=".marty.chapters.chapter1.Chapter1_1" />
        <activity android:name=".marty.chapters.chapter1.Chapter2_1" />

        <!-- Mark activity -->
        <activity android:name=".mark.MarkMainActivity" />
