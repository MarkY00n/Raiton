        <!-- Hani activity -->
        <activity android:name=".Hani.HaniMainActivity" />
        <activity android:name=".Hani.chapters.Chapter01" />
        <activity android:name=".mark.chapters.Chapter02"></activity>
        <activity android:name=".Hani.chapters.Chapter02"></activity>
    </application>

</manifest>